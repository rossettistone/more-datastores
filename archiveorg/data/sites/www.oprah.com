$('triptych_prev').observe('click', function() {
    var row = crow - 1;
    
    if ($$('.video_row')[row]) {
        $$('.video_row')[crow].hide();
        $$('.video_row')[row].show();
        $('triptych_next').show();
        if (!$$('.video_row')[row-1]) $('triptych_prev').hide();

        crow = row;
    }
});

$('triptych_next').observe('click', function() {
    var row = crow + 1;
    
    if ($$('.video_row')[row]) {
        $$('.video_row')[crow].hide();
        $$('.video_row')[row].show();
        $('triptych_prev').show();
        if (!$$('.video_row')[row+1]) $('triptych_next').hide();

        crow = row;
    }
});

if ($$('.video_row')[crow+1]) {
    $('triptych_next').show();
}

var has_video = !!document.createElement('video').canPlayType;
var has_touch = has_video && !!navigator.userAgent.match(/(?:iPhone|iPad)/i);

window.swfInit = function(data) {
  var vid = $(data.id);
  var el = vid.up('.video_box');
  var img = el.down('.image');

  el.hover({
    onOver: function() {
      img.hide();
      vid._playClip(false);
    },
    onOut: function() {
      img.show();
      vid._stopClip();
    }
  });

  vid._addEventListener("click", "onSwfClick");
};

window.onSwfClick = function(data) {
  var obj = $(data.id);
  var box = obj.up('.video_box');
  window.location = box.readAttribute('data-href');
};


$$('.video_box').each(function(el, i) {
  var href = el.readAttribute('data-href');
  var vid;

  el.down('.caption').observe('click', function() {
    window.location = href;
  });


  if (has_video) {
    vid = initHtmlVideo(el, href);
  }
  else {
    vid = initFlashVideo(el, i);
  }

  if (!vid) {
    el.down('.image').observe('click', function() {
      window.location = href;
    });
  }

});

function initHtmlVideo(el, href) {
  var vid = el.down('video');
  var img = el.down('.image');
  var initialized;

  if (!vid) return;

  if (has_touch) {
    img.observe('click', function() {
      img.hide();
      vid.play();
    })
    vid.observe('ended', function() {
      img.show();
    })
  }
  else {
    vid.observe('click', function() {
      window.location = href;
    });
    vid.observe('canplaythrough', function() {
      if (initialized) return;
      initialized = true;

      el.hover({
        onOver: function() {
          img.hide();
          vid.play();
        },
        onOut: function() {
          img.show();
          vid.pause();
          vid.currentTime = 0;
          vid.pause();
        }
      });
    })
  }

  vid.observe('touchend', function() {
    window.location =  el.readAttribute('data-href');
  })

  return vid;
}


function initFlashVideo(el, i) {
  var vid = el.down('.video');
  var videoSrc = el.readAttribute('data-src');

  if (videoSrc && vid) {
    var so = new SWFObject("http://www.oprah.com/player/ClipLooper.swf", 'video_' + i, 318, 408, "9.0.115", "#FFFFFF");
    so.addParam("allowFullScreen", "false");
    so.addParam("wmode", "transparent");
    so.addParam("allowscriptaccess", "always");
    so.addVariable("src", videoSrc);
    so.write(vid)
    return so;
  }
}

})();
</script>
